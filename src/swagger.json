                      "type": "string"
                    },
                    "email": {
                      "type": "string"
                    },
                    "cpf": {
                      "type": "string"
                    },
                    "sexo": {
                      "type": "string"
                    },
                    "estadoCivil": {
                      "type": "string"
                    },
                    "phone": {
